@model List<CityModel>

@{
    ViewBag.Title = Html.LocalizationString("home_screen_title");
}

<div class="col-lg-12">
    <h3>@ViewBag.Title</h3>
    <hr />

    @foreach (var item in Model.ToList())
    {
        <div class="col-lg-12">
            <a href="/city/districts/@item.Id" class="col-lg-3">
                <h2>
                    <span class="label-primary col-lg-4">@item.LicensePlate</span>
                    <span class="col-lg-4">@item.Name</span>
                </h2>
            </a>
    </div>
    } 
</div>


